{
  "name": "sveltekit-embed",
  "version": "0.0.5",
  "author": {
    "name": "Scott Spence",
    "email": "yo@scottspence.dev",
    "url": "https://scottspence.com"
  },
  "description": "This is a collection of embed components I use on a regular basis packaged up for use.",
  "keywords": [
    "svelte",
    "sveltekit",
    "youtube",
    "spotify",
    "deezer",
    "vimeo"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "pnpm un sveltekit-embed && pnpm i -D sveltekit-embed && vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "prepare": "svelte-kit sync",
    "test": "playwright test",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "pretty-quick",
    "package:local": "svelte-kit package && pnpm i -D ./package"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@playwright/test": "1.23.4",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@tailwindcss/typography": "0.5.4",
    "@typescript-eslint/eslint-plugin": "5.30.7",
    "@typescript-eslint/parser": "5.30.7",
    "autoprefixer": "10.4.7",
    "daisyui": "2.19.1",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-svelte3": "4.0.0",
    "husky": "8.0.1",
    "mdsvex": "0.10.6",
    "postcss": "8.4.14",
    "postcss-load-config": "4.0.1",
    "prettier": "2.7.1",
    "prettier-plugin-svelte": "2.7.0",
    "pretty-quick": "3.1.3",
    "svead": "0.0.3",
    "svelte": "3.49.0",
    "svelte-check": "2.8.0",
    "svelte-preprocess": "4.10.7",
    "svelte2tsx": "0.5.11",
    "sveltekit-embed": "0.0.5",
    "tailwindcss": "3.1.6",
    "tslib": "2.4.0",
    "typescript": "4.7.4",
    "vite": "3.0.2"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spences10/sveltekit-embed.git"
  },
  "bugs": {
    "url": "https://github.com/spences10/sveltekit-embed/issues"
  },
  "homepage": "https://github.com/spences10/sveltekit-embed#readme"
}
